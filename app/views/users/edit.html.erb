<div class="container">
  <h5>Edit User</h5>
    <%= form_for current_user do |f| %>

    <p>
      <%= f.label :first_name %>
      <%= f.text_field :first_name  %>
    </p>

    <p>
      <%= f.label :last_name %>
      <%= f.text_field :last_name %>
    </p>

    <p>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </p>

    <p>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </p>

    <p>
      <%= f.submit class: 'button tiny radius' %>
    </p>
    <%end%>

</div>